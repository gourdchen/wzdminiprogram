/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.anniu{
  float: left;
  width: 33%;
  height: 50px;
  text-align: center;
  border:0px solid gray;
  border-bottom: none;
  border-left: none;
}
.feet{
  height: 50px;
  width: 100%;
  position: fixed;
  bottom: 0px;
}
.head{
text-align: center;
  width: 100%;
  background-color: orange;
  margin-top: 0px;
  line-height: 50rpx;
  font-family: 'Trebuchet MS', 'Lucida Sans Unicode', 'Lucida Grande', 'Lucida Sans', Arial, sans-serif;
  font-size: 30rpx;
}
.flex{
  display: flex;
  padding: 0;
  margin: 0;
}
.icon{
  width: 30px;
  height: 30px;
}