}
.swiper-tab {  
  display: flex;  
  flex-direction: row;  
  line-height: 80rpx;  
  border-bottom: 1px solid #dbdbdb;  
  padding: 0;
  display: flex;
  justify-content: space-around;
}  
  
.tab-item {  
  width: 25%;  
  margin:0 1%; 
  text-align: center;  
  color: #777;  
  font-size: 30rpx;
}  
  
.swiper {  
  background: white;
  height: 1105rpx;
}
  
.on {  
  color: #1296db;  
  border-bottom: 5rpx solid #1296db;  
}  

scroll-view{
  height: 1100rpx;
}

.label{
