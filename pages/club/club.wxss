  margin-left: 40rpx;
}
.app_input{
  width: 95%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding: 10rpx 10rpx;
  border-bottom: solid 1rpx #dbdbdb;
}
input{
