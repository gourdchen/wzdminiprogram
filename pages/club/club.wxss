  justify-content: space-around;
  height: 200rpx;
  align-items: center;
}
.item{
  height: 100rpx;
  margin: 2rpx 0;
  border-bottom: solid 1rpx whitesmoke;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  background-color: whitesmoke;
  position: relative;
  padding: 10rpx;
}
.logo{
  width: 150rpx;
  height: 150rpx;
  margin: 50rpx 280rpx;
  padding: 0;
  

   border-radius: 10rpx;
   box-shadow:0 0 5rpx 3rpx #1296db; 
}
.icon{
